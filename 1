          or v.ClassName == "SpawnLocation"
          or v.ClassName == "WedgePart"
          or v.ClassName == "Terrain"
          or v.ClassName == "MeshPart" then
          v.BrickColor = BrickColor.new(155, 155, 155)
          v.Material = "Plastic"
          end
          if v.ClassName == "Decal" or v.ClassName == "Texture"  then
              v:Destroy()
          end
      end
  end
